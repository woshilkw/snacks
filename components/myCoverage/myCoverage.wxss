.cateWorld-showImg {
  height: 300rpx;
}
.cateWorld-showImg .showImg-img {
  width: 100%;
  height: 100%;
}
.cateWorld-imgs {
  display: flex;
  flex-wrap: wrap;
}
.cateWorld-imgs .imgs-item {
  height: 300rpx;
  background-color: #e1e3e1;
  width: 40%;
  margin-left: 50rpx;
  margin-top: 20rpx;
  border-radius: 10rpx;
  position: relative;
}
.cateWorld-imgs .imgs-item .item-img {
  width: 100%;
  height: 100%;
 
}
.cateWorld-imgs .imgs-item .item-img .img-img{
  margin-top: 20rpx;
  margin-left: 80rpx;
  width: 50%;
  height: 50%;
}
.cateWorld-imgs .imgs-item .item-view1 {
  position: absolute;
  left:60rpx;
  bottom: 60rpx;
  text-align: center;
}
.cateWorld-imgs .imgs-item .item-view2 {

  margin-top: -60rpx;
  text-align: center;
}